{
    "name": "example-typescript",
    "version": "0.1.0",
    "private": true,
    "webkit": {
        "plugin": true
    },
    "scripts": {
        "yarn:install": "yarn --silent",
        "start:dev": "react-scripts start",
        "analyze": "source-map-explorer 'build/static/js/*.js' --exclude-source-map --gzip",
        "build": "react-scripts build",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint": "tsc --noEmit && eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint:fix": "yarn lint --fix",
        "graphql:generate": "graphql-codegen --config codegen.yml",
        "i18next:parse": "i18next 'src/**/*.{ts,tsx}'"
    },
    "dependencies": {
        "@apollo/client": "^3.3.11",
        "@emotion/css": "^11.5.0",
        "@emotion/react": "^11.5.0",
        "@emotion/styled": "^11.3.0",
        "@mui/icons-material": "^5.8.4",
        "@mui/lab": "^5.0.0-alpha.98",
        "@mui/material": "^5.10.4",
        "@mui/styles": "^5.10.3",
        "@mui/system": "^5.10.6",
        "@mui/x-date-pickers": "^5.0.1",
        "apollo-upload-client": "^14.1.3",
        "axios": "^1.1.3",
        "clsx": "^1.1.1",
        "date-fns": "^2.28.0",
        "formik": "^2.1.5",
        "formik-mui": "^4.0.0-alpha.3",
        "formik-mui-lab": "^1.0.0-alpha.3",
        "graphql": "15.3.0",
        "i18next": "^19.8.7",
        "i18next-browser-languagedetector": "^6.0.1",
        "i18next-chained-backend": "^2.0.1",
        "i18next-http-backend": "^1.1.0",
        "i18next-localstorage-backend": "^3.1.2",
        "immer": "^9.0.6",
        "jwt-decode": "^2.2.0",
        "luxon": "^3.0.3",
        "quill-emoji": "^0.2.0",
        "react": "^17.0.2",
        "react-animate-on-scroll": "^2.1.5",
        "react-beautiful-dnd": "^13.1.1",
        "react-dom": "^17.0.2",
        "react-dropzone": "^11.2.0",
        "react-helmet": "^6.1.0",
        "react-html-table-to-excel": "^2.0.0",
        "react-i18next": "^11.8.5",
        "react-image-lightbox": "^5.1.4",
        "react-lottie": "^1.2.3",
        "react-phone-input-mui": "^2.9.4",
        "react-quill": "^2.0.0",
        "react-router-dom": "5.2.0",
        "react-scripts": "4.0.3",
        "react-slick": "^0.28.1",
        "react-tracked": "^1.7.5",
        "react-use": "^15.3.4",
        "slick-carousel": "^1.8.1",
        "slugify": "^1.6.5",
        "smoothscroll-polyfill": "^0.4.4",
        "yarn": "^1.22.17",
        "yup": "^0.32.11"
    },
    "optionalDependencies": {
        "fsevents": "2.1.3"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "1.20.1",
        "@graphql-codegen/introspection": "1.18.1",
        "@graphql-codegen/typescript": "1.21.0",
        "@graphql-codegen/typescript-operations": "1.17.14",
        "@graphql-codegen/typescript-react-apollo": "2.2.1",
        "@types/lodash": "^4.14.161",
        "@types/luxon": "^1.25.0",
        "@types/node": "^14.11.1",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.0",
        "@types/react-router-dom": "^5.1.5",
        "@types/yup": "^0.29.7",
        "@typescript-eslint/eslint-plugin": "^4.2.0",
        "@typescript-eslint/parser": "^4.2.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.28.0",
        "husky": "^4.3.0",
        "i18next-parser": "^3.6.0",
        "lint-staged": "^10.4.0",
        "prettier": "^2.1.2",
        "source-map-explorer": "^2.5.2",
        "typescript": "4.5.4"
    },
    "browserslist": {
        "production": [
            ">3%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "yarn format",
            "yarn lint:fix"
        ]
    }
}
